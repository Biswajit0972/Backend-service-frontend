{
    "builds": [
      {
        "src": "frontend/package.json",   
        "use": "@vercel/static-build"
      },
      {
        "src": "backend/api/**/*.js",     
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/index.js/(.*)",              
        "dest": "/backend/index.js/$1"
      },
      {
        "src": "/(.*)",                  
        "dest": "/frontend/dist/$1"
      }
    ]
  }
  